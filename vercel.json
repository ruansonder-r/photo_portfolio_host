{
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "photo_portfolio.settings"
  },
  "buildCommand": "./build.sh",
  "functions": {
    "api/index.py": {
      "runtime": "python@3.10",
      "maxDuration": 30,
      "memory": 1024
    }
  }
} 
